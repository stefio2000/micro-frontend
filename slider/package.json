{
  "name": "slider",
  "description": "A simple slider as a web component",
  "version": "1.0.0",
  "author": "St√©phane 'stefio2000' DAVID",
  "license": "MIT",
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration=production --output-hashing bundles",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean-sample-lib": "rm -f ../sample-app/lib/slider/* || true",
    "copy-to-sample": "for f in polyfills.*.js runtime.*.js main.*.js styles.*.css; do cp -rf dist/slider/$f ../sample-app/lib/slider/; done",
    "make-app": "npm run build && npm run clean-sample-lib && npm run copy-to-sample"
  },
  "dependencies": {
    "@angular/animations": "~13.3.5",
    "@angular/common": "~13.3.5",
    "@angular/compiler": "~13.3.5",
    "@angular/core": "~13.3.5",
    "@angular/elements": "^13.3.5",
    "@angular/forms": "~13.3.5",
    "@angular/platform-browser": "~13.3.5",
    "@angular/platform-browser-dynamic": "~13.3.5",
    "@angular/router": "~13.3.5",
    "@angular/material": "^13.3.5",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.4",
    "@angular/cli": "~13.3.4",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.2"
  }
}
